<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/mom_color1</item>
        <item name="colorPrimaryDark">@color/mom_color1</item>
        <item name="colorAccent">@color/color6</item>
        <item name="android:textColorPrimaryInverse">@color/color6</item>
        <item name="android:navigationBarColor">@color/mom_color1</item>
        <item name="android:textColorPrimary">@color/color6</item>
    </style>


    <style name="AppTheme.NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorAccent">@color/color6</item>
        <item name="colorPrimaryDark">@color/primary_dark_material_dark</item>
        <item name="colorPrimary">@color/primary_material_dark</item>
        <item name="android:colorBackground">@color/mom_color1</item>
        <item name="android:colorForeground">@android:color/background_dark</item>
        <item name="android:navigationBarColor">@android:color/black</item>
        <item name="android:textColorPrimary">@color/color6</item>
    </style>
</resources>